{
  "name": "@catalystsquad/ui-typescript-hello-app",
  "version": "1.0.0",
  "description": "Frontend app for the Catalyst Squad Hello World end to end workflow",
  "main": "app.ts",
  "scripts": {
    "build": "cp -a static/* dist/ && esbuild src/app.ts --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --outdir=dist",
    "dev": "cp -a static/* dist/ && esbuild src/app.ts --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --outdir=dist --watch --servedir=dist",
    "test": "test"
  },
  "keywords": [
    "helloworld",
    "minimalist",
    "esbuild",
    "vanilla",
    "typescript",
    "webcomponents"
  ],
  "author": "Catalyst Squad",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.17.11"
  }
}
